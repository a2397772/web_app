<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quinn.app.dao.BankDao">


    <resultMap id="BaseResultMap" type="com.quinn.app.model.entity.Bank" >
        <result column="id" property="id" jdbcType="bigint" />
        <result column="bankCode" property="bankCode" jdbcType="varchar" />
        <result column="bankName" property="bankName" jdbcType="varchar" />
        <result column="level" property="level" jdbcType="int" />
        <result column="publicFlag" property="publicFlag" jdbcType="int" />
        <result column="desc" property="desc" jdbcType="varchar" />
    </resultMap>

    <sql id="Base_Column_List" >
    id,bankCode,bankName,level,publicFlag,desc
    </sql>


    <select id="getById" resultType="com.quinn.app.model.entity.Bank">
        select * from tb_bank where id=#{id}
    </select>

    <insert id="insert" parameterType="com.quinn.app.model.entity.Bank" >
        insert into tb_bank (
          <include refid="Base_Column_List" />
        )
        values (
        #{id},#{bankCode},#{bankName},#{level},#{publicFlag},#{desc}
        )
    </insert>

    <update id="updateById" parameterType="com.quinn.app.model.entity.Bank" >
      update tb_bank
      <set>
        id = #{id}
        bankCode = #{bankCode}
        bankName = #{bankName}
        level = #{level}
        publicFlag = #{publicFlag}
        desc = #{desc}
      </set>
      where id = #{id}
    </update>

    <update id="updateByIdSelective" parameterType="com.quinn.app.model.entity.Bank" >
    update tb_bank
    <set>
        <if test="id != null" >
        id = #{id}
        </if>
        <if test="bankCode != null" >
        bankCode = #{bankCode}
        </if>
        <if test="bankName != null" >
        bankName = #{bankName}
        </if>
        <if test="level != null" >
        level = #{level}
        </if>
        <if test="publicFlag != null" >
        publicFlag = #{publicFlag}
        </if>
        <if test="desc != null" >
        desc = #{desc}
        </if>
    </set>
        where id = #{id}
    </update>


</mapper>